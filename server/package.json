{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend server for hooked-on-the-kootenays application",
  "main": "server.js",
  "exports": "./dist/server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require dotenv/config  __tests__/**/*.test.ts --import=tsx --exit",
    "test:watch": "NODE_ENV=test mocha --require dotenv/config --import=tsx --require source-map-support/register --inspect --exit --watch __tests__/**/*.test.ts",
    "start": "node dist/server.js",
    "dev": "nodemon --exec tsx --inspect server.ts",
    "clean": "rm -rf api/dist",
    "build": "npm run clean && tsc",
    "vercel-build": "echo we be buildin..."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/papaparse": "^5.3.15",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@google-cloud/storage": "^7.15.2",
    "@types/chai": "^5.2.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.3",
    "chai": "^5.2.0",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.9",
    "papaparse": "^5.5.2",
    "supertest": "^7.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
